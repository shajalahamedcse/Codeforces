

#include <iostream>
using namespace std;
typedef long long int ll;
int main()
{
    ll n,l;
    cin>>n;
    l=n%4;
    if(n==0)
        cout<<"1";
    else if(l==1)
        cout<<"8";
    else if(l==2)
        cout<<"4";
    else if(l==3)
        cout<<"2";
    else if(l==0)
        cout<<"6";
    return 0;
}
